{
  "name": "@appscode/vue-openapi-form",
  "version": "0.1.0",
  "main": "dist/vue-openapi-form.ssr.js",
  "browser": "dist/vue-openapi-form.esm.js",
  "module": "dist/vue-openapi-form.esm.js",
  "unpkg": "dist/vue-openapi-form.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue",
    "!src/App.vue",
    "!src/main.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "pkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "pkg:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "pkg:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "pkg:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "bulma": "^0.8.0",
    "bulma-switch": "^2.0.0",
    "core-js": "^3.3.2",
    "font-awesome": "^4.7.0",
    "js-yaml": "^3.13.1",
    "v-tooltip": "^2.0.2",
    "vee-validate": "^3.1.3",
    "vue": "^2.6.10",
    "vue-codemirror": "^4.0.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-external-helpers": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.18.2",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-scss": "^2.1.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  },
  "description": "A Vue component to generate html form using OpenAPI v3 schema",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appscode/vue-openapi-form.git"
  },
  "keywords": [
    "json-schema",
